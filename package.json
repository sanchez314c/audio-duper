{
  "name": "audio-dedupe",
  "version": "1.0.0",
  "description": "Audio duplicate finder using chromaprint",
  "author": "AudioDUPER Team",
  "main": "main.js",
  "homepage": "https://github.com/audiodupe/audiodupe",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux",
    "dist": "npm run build",
    "dist:current": "electron-builder",
    "dist:mac": "electron-builder --mac",
    "dist:win": "electron-builder --win", 
    "dist:linux": "electron-builder --linux",
    "dist:all": "npm run dist:mac && npm run dist:win && npm run dist:linux",
    "dist:win:msi": "electron-builder --win --x64 --ia32 msi",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "pack:mac": "electron-builder --mac --dir",
    "pack:win": "electron-builder --win --dir",
    "pack:linux": "electron-builder --linux --dir",
    "test": "echo \"No tests yet\" && exit 0",
    "lint": "echo \"No linting configured\" && exit 0",
    "clean": "rm -rf dist build node_modules/.cache out release",
    "clean:deep": "npm run clean && rm -rf node_modules package-lock.json",
    "setup": "./install.sh || install.bat",
    "bloat-check": "./bloat-check.sh",
    "temp-clean": "./temp-cleanup.sh",
    "build-comprehensive": "./compile-build-dist-comprehensive.sh",
    "run:source:mac": "./run-macos-source.sh",
    "run:source:linux": "./run-linux-source.sh",
    "run:built:mac": "./run-macos.sh", 
    "run:built:linux": "./run-linux.sh"
  },
  "dependencies": {
    "fpcalc": "^1.3.0",
    "music-metadata": "^7.14.0"
  },
  "devDependencies": {
    "electron": "^28.0.0",
    "electron-builder": "^24.0.0"
  },
  "build": {
    "appId": "com.audiodedupe.app",
    "productName": "AudioDUPER",
    "copyright": "Copyright Â© 2024 AudioDUPER Team. All rights reserved.",
    "icon": "build-resources/icon",
    "directories": {
      "output": "dist",
      "buildResources": "build-resources"
    },
    "files": [
      "main.js",
      "preload.js",
      "index.html",
      "package.json",
      "node_modules/fpcalc/index.js",
      "node_modules/fpcalc/package.json",
      "node_modules/fpcalc/bin/**",
      "node_modules/music-metadata/**",
      "!node_modules/**/test/**",
      "!node_modules/**/tests/**",
      "!node_modules/**/spec/**",
      "!node_modules/**/docs/**",
      "!node_modules/**/doc/**",
      "!node_modules/**/example/**",
      "!node_modules/**/examples/**",
      "!node_modules/**/demo/**",
      "!node_modules/**/*.md",
      "!node_modules/**/*.map",
      "!node_modules/**/*.ts",
      "!node_modules/**/tsconfig.json",
      "!node_modules/**/rollup.config.js",
      "!node_modules/**/webpack.config.js",
      "!node_modules/**/gulpfile.js",
      "!node_modules/**/gruntfile.js",
      "!node_modules/**/bower.json",
      "!node_modules/**/yarn.lock",
      "!node_modules/**/package-lock.json",
      "!node_modules/**/.gitignore",
      "!node_modules/**/.eslintrc*",
      "!node_modules/**/.jshintrc*",
      "!node_modules/**/.prettierrc*",
      "!node_modules/**/jest.config.*",
      "!node_modules/**/*.test.js",
      "!node_modules/**/*.spec.js"
    ],
    "extraFiles": [],
    "asarUnpack": [
      "node_modules/fpcalc/bin/**"
    ],
    "compression": "maximum",
    "removePackageScripts": true,
    "publish": null,
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        {
          "target": "dmg",
          "arch": ["x64", "arm64"]
        },
        {
          "target": "zip",
          "arch": ["x64", "arm64"]
        }
      ],
      "icon": "build-resources/icon.icns",
      "artifactName": "${productName}-${version}-mac-${arch}.${ext}",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "identity": null,
      "entitlements": "build-resources/entitlements.mac.plist",
      "entitlementsInherit": "build-resources/entitlements.mac.plist",
      "minimumSystemVersion": "10.15.0",
      "type": "distribution"
    },
    "dmg": {
      "title": "${productName} ${version}",
      "artifactName": "${productName}-${version}-installer.${ext}",
      "icon": "build-resources/icon.icns",
      "iconSize": 80,
      "window": {
        "width": 540,
        "height": 380
      },
      "contents": [
        {
          "x": 140,
          "y": 200
        },
        {
          "x": 400,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "backgroundColor": "#2c3e50",
      "internetEnabled": false,
      "sign": false
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": ["x64", "ia32"]
        },
        {
          "target": "portable",
          "arch": ["x64"]
        }
      ],
      "icon": "build-resources/icon.ico",
      "artifactName": "${productName}-${version}-win-${arch}.${ext}",
      "publisherName": "AudioDUPER Team",
      "verifyUpdateCodeSignature": false,
      "rfc3161TimeStampServer": "http://timestamp.digicert.com",
      "timeStampServer": "http://timestamp.digicert.com",
      "certificateFile": null,
      "certificatePassword": null
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "${productName}",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "deleteAppDataOnUninstall": false,
      "menuCategory": "Audio & Video",
      "include": null,
      "script": null,
      "installerIcon": "build-resources/icon.ico",
      "uninstallerIcon": "build-resources/icon.ico",
      "installerHeaderIcon": "build-resources/icon.ico",
      "installerSidebar": null,
      "uninstallerSidebar": null,
      "warningsAsErrors": false
    },
    "msi": {
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "oneClick": false,
      "perMachine": false,
      "upgradeCode": "{12345678-1234-5678-9ABC-123456789ABC}"
    },
    "portable": {
      "artifactName": "${productName}-${version}-${arch}-portable.${ext}"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": ["x64"]
        },
        {
          "target": "deb",
          "arch": ["x64"]
        },
        {
          "target": "rpm",
          "arch": ["x64"]
        }
      ],
      "icon": "build-resources/",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "category": "AudioVideo",
      "description": "Audio duplicate finder using chromaprint fingerprinting",
      "desktop": {
        "Name": "AudioDUPER",
        "Comment": "Audio duplicate finder",
        "Icon": "audiodupder",
        "Type": "Application",
        "Categories": "AudioVideo;Audio;Music;",
        "MimeType": "audio/mpeg;audio/x-mp3;audio/mp3;audio/x-mpeg;audio/mpeg3;audio/x-mpeg-3;audio/mpg;audio/x-mpg;audio/x-mpegaudio;audio/flac;audio/x-flac;audio/ogg;audio/x-vorbis+ogg;audio/x-ogg;audio/mp4;audio/x-m4a;audio/wav;audio/x-wav;",
        "StartupNotify": "true"
      },
      "maintainer": "AudioDUPER Team",
      "vendor": "AudioDUPER Team",
      "synopsis": "Audio duplicate finder using chromaprint"
    },
    "deb": {
      "packageCategory": "sound",
      "depends": ["libchromaprint-tools", "gconf2", "gconf-service", "libxss1", "libgconf-2-4", "libxrandr2", "libasound2", "libpangocairo-1.0-0", "libatk1.0-0", "libcairo1", "libgtk-3-0", "libgdk-pixbuf2.0-0", "libxcomposite1", "libxcursor1", "libxdamage1", "libxext6", "libxfixes3", "libxi6", "libxrender1", "libxtst6", "libatspi2.0-0", "libdrm2", "libxss1", "libgtk-3-0"]
    },
    "rpm": {
      "packageCategory": "Applications/Multimedia",
      "depends": ["chromaprint-tools"]
    },
    "snap": {
      "summary": "Audio duplicate finder",
      "description": "AudioDUPER uses advanced audio fingerprinting to identify and remove duplicate audio files from your music collection.",
      "grade": "stable",
      "confinement": "strict",
      "buildPackages": ["build-essential"],
      "stagePackages": ["libnss3-dev", "libatk-bridge2.0-dev", "libdrm2", "libxcomposite1", "libxdamage1", "libxrandr2", "libgbm1", "libxss1", "libasound2-dev"],
      "plugs": ["default", "audio-playback", "home", "removable-media"],
      "environment": {
        "DISABLE_WAYLAND": "1"
      }
    },
    "appImage": {
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    }
  }
}