{
  "name": "audio-dedupe",
  "version": "1.0.0",
  "description": "Audio duplicate finder using chromaprint",
  "author": "Jasonn Michaels",
  "main": "src/main.js",
  "homepage": "https://github.com/sanchez314c/audio-duper",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "build:mac": "electron-builder --mac",
    "build:win": "electron-builder --win",
    "build:linux": "electron-builder --linux",
    "dist": "npm run build",
    "dist:current": "electron-builder",
    "dist:mac": "electron-builder --mac",
    "dist:win": "electron-builder --win",
    "dist:linux": "electron-builder --linux",
    "dist:all": "npm run dist:mac && npm run dist:win && npm run dist:linux",
    "dist:maximum": "npm run build && electron-builder --mac --win --linux --x64 --arm64 --ia32 && electron-builder --linux --armv7l && electron-builder --win msi",
    "build-clean": "./scripts/build-compile-dist.sh --no-temp-clean --no-bloat-check",
    "dist:win:msi": "electron-builder --win --x64 --ia32 msi",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "pack:mac": "electron-builder --mac --dir",
    "pack:win": "electron-builder --win --dir",
    "pack:linux": "electron-builder --linux --dir",
    "test": "jest --detectOpenHandles --forceExit",
    "test:watch": "jest --watch --detectOpenHandles",
    "test:coverage": "jest --coverage --detectOpenHandles --forceExit",
    "test:unit": "jest tests/unit --detectOpenHandles --forceExit",
    "test:integration": "jest tests/integration --detectOpenHandles --forceExit",
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint src --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{ts,tsx,js,jsx,json,css,md}",
    "clean": "rm -rf dist build node_modules/.cache out release",
    "clean:deep": "npm run clean && rm -rf node_modules package-lock.json",
    "setup": "./scripts/install.sh || scripts/install.bat",
    "bloat-check": "./scripts/bloat-check.sh",
    "temp-clean": "./scripts/temp-cleanup.sh",
    "run:source:mac": "./scripts/run-macos-source.sh",
    "run:source:linux": "./scripts/run-linux-source.sh"
  },
  "dependencies": {
    "fpcalc": "^1.3.0",
    "music-metadata": "^11.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "babel-jest": "^30.2.0",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "build": {
    "appId": "com.audiodedupe.app",
    "productName": "AudioDUPER",
    "copyright": "Copyright Â© 2024 Jasonn Michaels. All rights reserved.",
    "icon": "build_resources/icons/icon",
    "directories": {
      "output": "dist",
      "buildResources": "build_resources"
    },
    "files": [
      "src/main.js",
      "src/main/main.js",
      "src/preload.js",
      "src/preload/preload.js",
      "src/renderer/index.html",
      "src/shared/**",
      "package.json",
      "node_modules/fpcalc/index.js",
      "node_modules/fpcalc/package.json",
      "node_modules/fpcalc/bin/**",
      "node_modules/music-metadata/**",
      "!node_modules/**/test/**",
      "!node_modules/**/tests/**",
      "!node_modules/**/spec/**",
      "!node_modules/**/docs/**",
      "!node_modules/**/doc/**",
      "!node_modules/**/example/**",
      "!node_modules/**/examples/**",
      "!node_modules/**/demo/**",
      "!node_modules/**/*.md",
      "!node_modules/**/*.map",
      "!node_modules/**/*.ts",
      "!node_modules/**/tsconfig.json",
      "!node_modules/**/rollup.config.js",
      "!node_modules/**/webpack.config.js",
      "!node_modules/**/gulpfile.js",
      "!node_modules/**/gruntfile.js",
      "!node_modules/**/bower.json",
      "!node_modules/**/yarn.lock",
      "!node_modules/**/package-lock.json",
      "!node_modules/**/.gitignore",
      "!node_modules/**/.eslintrc*",
      "!node_modules/**/.jshintrc*",
      "!node_modules/**/.prettierrc*",
      "!node_modules/**/jest.config.*",
      "!node_modules/**/*.test.js",
      "!node_modules/**/*.spec.js"
    ],
    "extraFiles": [],
    "asarUnpack": [
      "node_modules/fpcalc/bin/**"
    ],
    "compression": "maximum",
    "removePackageScripts": true,
    "publish": null,
    "mac": {
      "category": "public.app-category.utilities",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "pkg",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "icon": "build_resources/icons/icon.icns",
      "artifactName": "${productName}-${version}-mac-${arch}.${ext}",
      "darkModeSupport": true,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "identity": null,
      "entitlements": "build_resources/entitlements.mac.plist",
      "entitlementsInherit": "build_resources/entitlements.mac.plist",
      "minimumSystemVersion": "10.15.0",
      "type": "distribution"
    },
    "dmg": {
      "title": "${productName} ${version}",
      "artifactName": "${productName}-${version}-installer.${ext}",
      "icon": "build_resources/icons/icon.icns",
      "iconSize": 80,
      "window": {
        "width": 540,
        "height": 380
      },
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ],
      "backgroundColor": "#2c3e50",
      "internetEnabled": false,
      "sign": false
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        }
      ],
      "icon": "build_resources/icons/icon-256x256.png",
      "artifactName": "${productName}-${version}-win-${arch}.${ext}",
      "verifyUpdateCodeSignature": false
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "${productName}",
      "artifactName": "${productName}-Setup-${version}.${ext}",
      "deleteAppDataOnUninstall": false,
      "menuCategory": "Audio & Video",
      "include": null,
      "script": null,
      "installerIcon": "build_resources/icons/icon-256x256.png",
      "uninstallerIcon": "build_resources/icons/icon-256x256.png",
      "installerHeaderIcon": "build_resources/icons/icon-256x256.png",
      "installerSidebar": null,
      "uninstallerSidebar": null,
      "warningsAsErrors": false
    },
    "msi": {
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "oneClick": false,
      "perMachine": false,
      "upgradeCode": "{12345678-1234-5678-9ABC-123456789ABC}"
    },
    "portable": {
      "artifactName": "${productName}-${version}-${arch}-portable.${ext}"
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "snap",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        }
      ],
      "icon": "build_resources/icons/icon-256x256.png",
      "artifactName": "${productName}-${version}-linux-${arch}.${ext}",
      "category": "AudioVideo"
    },
    "appImage": {
      "artifactName": "${productName}-${version}-${arch}.${ext}"
    }
  }
}
